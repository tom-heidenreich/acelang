const int = 1
printf("Integer: %d\n", $int)

const float = 2.1
printf("Float: %f\n", $float)

const string = "hello"
printf("String: %s\n", $string)

const bool = true
printf("Boolean: %d\n", ($bool) as int)

const array = [1, 2, 3]
array[0] = $array[2]
printf("Array Item: %d\n", $array[0])

const structs = {
    0: 1
    1: 2.1
}
printf("Struct Item: %d\n", $structs[0])