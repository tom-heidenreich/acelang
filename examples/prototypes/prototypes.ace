import { getStrLength, getCharAt } from 'strings'

# String prototype could look like this
const StringPrototype = {
    _constructor: (str: string) => {
        const obj = {
            length: () => {
                return getStrLength(str)
            },
            charAt: (index: int) => {
                return getCharAt(str, index)
            },
        }
        return obj
    }
}

const hello = ($StringPrototype._constructor)("hello")
printf('%d\n', ($($hello).length)())