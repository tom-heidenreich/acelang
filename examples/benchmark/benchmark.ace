import { current_milliseconds } from 'time'

func benchmark(n: int) {
    var sum = 0
    var i = 0
    while($i < n) {
        sum += $i
        i = $i + 1
    }
    return sum
}
const startTime = current_milliseconds()
benchmark(100000000)
printf('Took %dms to run', current_milliseconds() - $startTime)